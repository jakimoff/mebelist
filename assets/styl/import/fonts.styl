@font-face {
  font-family: 'Proxima Nova';
  src: url('~static/fonts/ProximaNova-Bold.eot');
  src: url('~static/fonts/ProximaNova-Bold.eot?#iefix') format('embedded-opentype'),
  url('~static/fonts/ProximaNova-Bold.woff2') format('woff2'),
  url('~static/fonts/ProximaNova-Bold.woff') format('woff'),
  url('~static/fonts/ProximaNova-Bold.ttf') format('truetype');
  font-weight: bold;
  font-style: normal;
  font-display: swap;
}

